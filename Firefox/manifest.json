{
  "manifest_version": 2,

  "name": "NCBI RookieUI",
  "version": "0.1.1",
  "description": "A new fresh style for NCBI website",
  "author": "Pierre JACQUET",
  "page_action": {
     "default_icon": "ROOKIE16.png"
   },
  "applications": {
  "gecko": {
     "id": "ncbi@rookieui.com",
    "update_url": "https://github.com/pierrejacquet/NCBI-RookieUI/blob/master/xpi-files/updates.json"
  }
},
  "homepage_url": "https://github.com/pierrejacquet/NCBI-RookieUI",
  "content_scripts": [{
    "css": ["styles.css","font-awesome.min.css"],
    "js": ["jquery-3.2.1.min.js","content.js"],
    "matches": ["https://www.ncbi.nlm.nih.gov/*"]
  },
  {
    "matches": ["https://www.ncbi.nlm.nih.gov/gene/*"],
    "css": ["styles.css","stylegene.css","font-awesome.min.css"],
    "js": ["jquery-3.2.1.min.js","content.js"]
  },
  {
    "matches": ["https://www.ncbi.nlm.nih.gov/nuccore/*"],
